generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



enum OrderStatus {
  NEW
  PAID
  COMPLETED
  CANCELED
}

enum DeliveryMethod {
  EMAIL
  AUTOMATIC
}

enum StatusOffer {
  ACTIVE
  INACTIVE
  SOLD
}

enum RolesPossible {
  ADMIN
  USER
  SUPPORT
  PREMIUM
}
