generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

enum OrderStatus {
  NEW
  PAID
  COMPLETED
  CANCELED
  REFUND_PENDING
  REFUNDED
  PARTIALLY_REFUNDED
}

enum DeliveryMethod {
  EMAIL
  AUTOMATIC
}

enum StatusOffer {
  ACTIVE
  INACTIVE
  SOLD
}

enum RolesPossible {
  ADMIN
  USER
  SUPPORT
  PREMIUM
}

enum ProtectionType {
  BASIC
  PREMIUM
  ADVANCED
  OTHER
}

enum NotificationType {
  SUCCESS
  ERROR
  WARNING
}

enum BuilderProcessStatus {
  WAITING
  IN_PROGRESS
  COMPLETED
  FAILED
}

enum BuilderStageType {
  ANALYSIS
  AUTHENTICATION
  CONFIGURE_SERVER
  ROLES_CREATE
  CHANNELS_CREATE
  CATEGORIES_CREATE
}

enum TemplateCategory {
  ALL
  ROLEPLAY
  THEMATIC
  ENGLISH
  SOCIAL
  NSFW
  GROUPS_OF_PEOPLE
  GUILDS
  MEMES
  SCAM
  ANIME
  GTA
  DEV
  MINECRAFT
  GAMING
  ADVERTISING
  AI
}
